<!-- For Seamonkey/Mozilla Suite compatibility -->

<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:chrome="http://www.mozilla.org/rdf/chrome#">

    <Seq about="urn:mozilla:package:root">
        <li resource="urn:mozilla:package:foxyproxy"/>
    </Seq>

    <Description about="urn:mozilla:package:foxyproxy"
        chrome:name="foxyproxy"
        chrome:displayName="FoxyProxy"
        chrome:author="Eric H. Jung"
        chrome:authorURL="http://foxyproxy.mozdev.org/"
        chrome:description="Proxy your life."
        chrome:xpcNativeWrappers="no"
        chrome:extension="true"
        chrome:settingsURL="chrome://foxyproxy/content/options.xul"
        chrome:iconURL="chrome://foxyproxy/content/images/foxyproxy-nocopy.gif">
    </Description>

    <Seq about="urn:mozilla:overlays">
        <li resource="chrome://navigator/content/navigator.xul"/>
        <li resource="chrome://communicator/content/alerts/alert.xul"/>
        <li resource="chrome://messenger/content/messenger.xul"/>
    </Seq>
 
    <Seq about="chrome://navigator/content/navigator.xul">
        <li>chrome://foxyproxy/content/suiteOverlay.xul</li>
    </Seq>
    <Seq about="chrome://communicator/content/alerts/alert.xul">
        <li>chrome://foxyproxy/content/alertOverlay.xul</li>
    </Seq>
    <Seq about="chrome://messenger/content/messenger.xul">
        <li>chrome://foxyproxy/content/tbirdOverlay.xul</li>
    </Seq>
 
</RDF>