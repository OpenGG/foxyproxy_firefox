<?xml-stylesheet href="foxyproxy.css" type="text/css"?>
<!DOCTYPE overlay SYSTEM "chrome://foxyproxy/locale/foxyproxy.dtd">

<dialog id="advancedSettings" onload="onLoad();" ondialogaccept="return onOK();"
  title="&foxyproxy.standard.label; - &foxyproxy.settings.advanced.label;"
  ondialogcancel="onCancel();" buttons="accept, cancel"
  xmlns="http://www.mozilla.org/keymaster/gatekeeper/there.is.only.xul"
  persist="screenX screenY width height">

  <script type="application/x-javascript;version=1.7"
    src="advancedSettings.js"/>

  <box>
    <description id="versionCheckString"/>
  </box>
  <box>
    <menulist id="proxyForVersionCheckMenu"
      tooltiptext="&foxyproxy.versioncheck.proxy.tooltip;">
      <menupopup/>
    </menulist>
  </box>
</dialog>
